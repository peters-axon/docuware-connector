      "config" : {
        "method" : "PUT",
        "target" : {
          "clientId" : "02d1eec1-32e9-4316-afc3-793448486203",
          "path" : "/FileCabinets/{FileCabinetId}/Documents/{DocumentId}/Fields",
          "templateParams" : {
